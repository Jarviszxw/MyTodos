{
  "name": "mytodos",
  "version": "1.0.0",
  "description": "Todo application built with React and Express",
  "main": "src/backend/server.js",
  "scripts": {
    "start": "node src/backend/server.js",
    "dev:backend": "nodemon src/backend/server.js",
    "dev:frontend": "cd src/frontend && PORT=3001 npm start",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:build-backend": "cd src/frontend && npm run build && cd ../.. && npm run dev:backend",
    "test": "jest",
    "build": "cd src/frontend && npm run build",
    "init-db": "node src/backend/scripts/init-db.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "openai": "^4.89.0",
    "pg": "^8.9.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "jest": "^29.4.3",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "todo",
    "express",
    "react",
    "postgresql",
    "rest-api"
  ],
  "author": "jarviszxw1024@gmail.com",
  "license": "MIT"
}
